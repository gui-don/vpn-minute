{
  "name": "gui-don/vpn-minute",
  "type": "library",
  "description": "On-demand/disposable VPN in a minute",
  "license": "GPL-3.0",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">7.3, <=8.0",
    "ext-mbstring": "*",

    "symfony/config": "^5",
    "symfony/dependency-injection": "^5",
    "symfony/yaml": "^5",
    "symfony/finder": "^5",
    "gui-don/rico-library": "^3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2",
    "pheromone/phpcs-security-audit": "^2.0",
    "phpunit/phpunit": "^9",
    "phpunit/php-code-coverage": "^9",
    "phpstan/phpstan": "^0.12.0",
    "behat/behat": "^3.8@dev",
    "phpspec/phpspec": "^7.0",
    "friends-of-phpspec/phpspec-code-coverage": "^6"
  },
  "autoload": {
    "psr-4": {
      "VPNMinute\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VPNMinute\\": "src/",
      "Spec\\VPNMinute\\": "test/Spec"
    }
  }
}
